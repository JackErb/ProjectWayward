script "UTILT";

func Init() {
    Hitbox_Circle(0, 0., - 200., 140.);
    Hitbox_Damage(0, 10.);
    Hitbox_Knockback(0, 90, 60., 30.);
}

func ProcessInput() {
    switch (frame) {
    case 0:
        NullVelocity();
    case 8:
        Hitbox_Spawn(0);
    case 11:
        Hitbox_Remove(0);
    case 20:
        quit();
    }
}

func Tick() {

}

func Airborne() {
    Hitbox_Clear();
    setState("AirborneNeutralState");
}
