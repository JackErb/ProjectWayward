script "NAIR";

func Init() {
    Hitbox_Circle(0, 125., 80., 140.);
    Hitbox_Circle(1, 125., 80., 100.);
}

func ProcessInput() {
    vector();
    gravity(0.9);
    
    y = 1.5;
    
    switch frame {
    case 5:
        Hitbox_Spawn(0);
    case 19:
        Hitbox_Remove(0);
        Hitbox_Spawn(1);
    case 26:
        Hitbox_Remove(1);
    case 36:
        quit();
    }
}

func Tick() {
    rotate();
}

func Land() {
    rotate0();
    Hitbox_Clear();
    setState("LandingLag", 10);
}
